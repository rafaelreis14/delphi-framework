unit Lib.Interfaces.CRUDBase;

interface

uses
    Classes, Lib.Enumeradores, Lib.Parametro, Lib.ResultFilter, Lib.Lista,
    Lib.Base;
type
    ICRUDBase = interface
        function Inserir: integer;
        function Alterar: integer;
        function Deletar: integer;
        function Listar(tipoOperacao: TOparationTypes; MultiSelect: Boolean): TResultFilter;
    end;


implementation

end.
