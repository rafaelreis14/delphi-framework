  object sqlTableInfo: TADOQuery
    Connection = ConexaoPrincipal
    CursorType = ctStatic
    Parameters = <
      item
        Name = 'Table'
        DataType = ftWideString
        NumericScale = 255
        Precision = 255
        Size = 128
        Value = Null
      end>
    SQL.Strings = (
      'select   '
      '    col.column_name, '
      '    col.data_type,'
      ' col.character_maximum_length,'
      '    prop.value as property'
      'FROM sys.objects o  '
      'INNER JOIN INFORMATION_SCHEMA.COLUMNS col '
      '    ON col.table_name = o.name'
      'LEFT JOIN sys.extended_properties prop '
      '    ON prop.major_id = o.object_id'
      '    and prop.minor_id = col.ORDINAL_POSITION'
      'WHERE o.name = :Table')
    Left = 632
    Top = 88
    object sqlTableInfocolumn_name: TWideStringField
      FieldName = 'column_name'
      Size = 128
    end
    object sqlTableInfodata_type: TWideStringField
      FieldName = 'data_type'
      ReadOnly = True
      Size = 128
    end
    object sqlTableInfocharacter_maximum_length: TIntegerField
      FieldName = 'character_maximum_length'
      ReadOnly = True
    end
    object sqlTableInfoproperty: TVariantField
      FieldName = 'property'
    end
  end
